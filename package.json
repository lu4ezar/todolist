{
	"name": "todolist",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@apollo/client": "3.6.1",
		"@mui/icons-material": "^5.6.2",
		"@mui/lab": "^5.0.0-alpha.79",
		"@mui/material": "^5.6.3",
		"@mui/styled-engine-sc": "^5.6.1",
		"@mui/styles": "^5.6.2",
		"@testing-library/jest-dom": "5.16.4",
		"@emotion/react": "^11.9.0",
		"@emotion/styled": "^11.8.1",
		"formik": "^2.2.9",
		"graphql": "^16.4.0",
		"jwt-decode": "^3.1.2",
		"luxon": "^2.3.2",
		"phosphor-react": "^1.4.1",
		"react": "18.0.0",
		"react-beautiful-dnd": "13.1.0",
		"react-dom": "18.0.0",
		"react-scripts": "5.0.1",
		"styled-components": "^5.3.5",
		"uuid": "8.3.2"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test --env=jsdom",
		"predeploy": "npm run generate && npm run build",
		"deploy": "gh-pages -d build",
		"eject": "react-scripts eject",
		"flow": "flow",
		"lint": "eslint ./src/**/*.js[x] && flow",
		"generate": "graphql-codegen --config codegen.yml",
		"storybook": "start-storybook -p 6006 -s public",
		"build-storybook": "build-storybook -s public"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"devDependencies": {
		"@babel/cli": "7.17.10",
		"@babel/core": "7.17.10",
		"@babel/preset-flow": "7.16.7",
		"@graphql-codegen/cli": "^2.6.2",
		"@graphql-codegen/flow": "2.2.8",
		"@graphql-codegen/flow-operations": "2.2.8",
		"@graphql-codegen/flow-resolvers": "2.2.9",
		"@graphql-codegen/introspection": "2.1.1",
		"@react-mock/localstorage": "0.1.2",
		"@storybook/addon-actions": "^6.4.22",
		"@storybook/addon-essentials": "^6.4.22",
		"@storybook/addon-knobs": "^6.4.0",
		"@storybook/addon-links": "^6.4.22",
		"@storybook/addon-storyshots": "^6.4.22",
		"@storybook/node-logger": "^6.4.22",
		"@storybook/preset-create-react-app": "^4.1.0",
		"@storybook/react": "^6.5.0-beta.1",
		"@testing-library/react": "13.1.1",
		"@testing-library/user-event": "^14.1.1",
		"babel-plugin-styled-components": "2.0.7",
		"eslint-config-airbnb": "19.0.4",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-flowtype": "8.0.3",
		"eslint-plugin-import": "2.26.0",
		"eslint-plugin-jest-dom": "^4.0.1",
		"eslint-plugin-jsx-a11y": "6.5.1",
		"eslint-plugin-prettier": "4.0.0",
		"eslint-plugin-react": "7.29.4",
		"flow-bin": "0.176.3",
		"eslint-plugin-react-hooks": "4.5.0",
		"flow-typed": "^3.7.0",
		"gh-pages": "3.2.3",
		"husky": "7.0.4",
		"lint-staged": "12.4.1",
		"prettier": "2.6.2",
		"react-test-renderer": "^18.0.0",
		"storybook-addon-apollo-client": "^4.0.11"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push": "CI=true npm run test"
		}
	},
	"lint-staged": {
		"*.+(js|jsx)": [
			"eslint src/**/*.js[x] --fix"
		],
		"*.+(js|jsx|json|css|md)": [
			"prettier --write"
		]
	}
}