{
	"name": "todolist",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@apollo/client": "3.3.20",
		"@material-ui/core": "4.11.4",
		"@material-ui/icons": "4.11.2",
		"@material-ui/lab": "4.0.0-alpha.58",
		"@material-ui/styles": "4.11.4",
		"@testing-library/jest-dom": "5.14.1",
		"formik": "^2.2.9",
		"graphql": "15.5.1",
		"jwt-decode": "^3.1.2",
		"luxon": "^1.27.0",
		"phosphor-react": "^1.2.1",
		"react": "17.0.1",
		"react-beautiful-dnd": "13.1.0",
		"react-dom": "17.0.1",
		"react-scripts": "4.0.3",
		"styled-components": "5.3.0",
		"uuid": "8.3.2"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test --env=jsdom",
		"predeploy": "npm run generate && npm run build",
		"deploy": "gh-pages -d build",
		"eject": "react-scripts eject",
		"flow": "flow",
		"lint": "eslint ./src/**/*.js[x] && flow",
		"generate": "graphql-codegen --config codegen.yml",
		"storybook": "start-storybook -p 6006 -s public",
		"build-storybook": "build-storybook -s public"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"devDependencies": {
		"@babel/cli": "7.14.5",
		"@babel/core": "7.14.6",
		"@babel/preset-flow": "7.14.5",
		"@graphql-codegen/cli": "1.21.6",
		"@graphql-codegen/flow": "1.19.4",
		"@graphql-codegen/flow-operations": "1.18.12",
		"@graphql-codegen/flow-resolvers": "1.17.17",
		"@graphql-codegen/introspection": "1.18.2",
		"@react-mock/localstorage": "0.1.2",
		"@storybook/addon-actions": "^6.3.0",
		"@storybook/addon-essentials": "^6.3.0",
		"@storybook/addon-knobs": "^6.2.9",
		"@storybook/addon-links": "^6.3.0",
		"@storybook/node-logger": "^6.3.0",
		"@storybook/preset-create-react-app": "^3.1.7",
		"@storybook/react": "^6.3.0",
		"@testing-library/react": "12.0.0",
		"@testing-library/user-event": "^13.1.9",
		"babel-eslint": "10.1.0",
		"babel-plugin-styled-components": "1.13.1",
		"eslint-config-airbnb": "18.2.1",
		"eslint-config-prettier": "8.3.0",
		"eslint-config-react-app": "6.0.0",
		"eslint-plugin-flowtype": "5.7.2",
		"eslint-plugin-import": "2.23.4",
		"eslint-plugin-jest-dom": "^3.9.0",
		"eslint-plugin-jsx-a11y": "6.4.1",
		"eslint-plugin-prettier": "3.4.0",
		"eslint-plugin-react": "7.24.0",
		"eslint-plugin-react-hooks": "4.2.0",
		"flow-bin": "0.154.0",
		"flow-typed": "3.3.1",
		"gh-pages": "3.2.3",
		"husky": "6.0.0",
		"lint-staged": "11.0.0",
		"prettier": "2.3.2",
		"react-test-renderer": "^17.0.1",
		"storybook-addon-apollo-client": "^4.0.8"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push": "CI=true npm run test"
		}
	},
	"lint-staged": {
		"*.+(js|jsx)": [
			"eslint src/**/*.js[x] --fix"
		],
		"*.+(js|jsx|json|css|md)": [
			"prettier --write"
		]
	}
}
