{
	"name": "todolist",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@apollo/client": "3.5.6",
		"@material-ui/core": "4.12.3",
		"@material-ui/icons": "4.11.2",
		"@material-ui/lab": "4.0.0-alpha.60",
		"@material-ui/styles": "4.11.4",
		"@testing-library/jest-dom": "5.16.1",
		"formik": "^2.2.9",
		"graphql": "^16.0.1",
		"jwt-decode": "^3.1.2",
		"luxon": "^2.2.0",
		"phosphor-react": "^1.3.1",
		"react": "17.0.2",
		"react-beautiful-dnd": "13.1.0",
		"react-dom": "17.0.2",
		"react-scripts": "4.0.3",
		"styled-components": "5.3.3",
		"uuid": "8.3.2"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test --env=jsdom",
		"predeploy": "npm run generate && npm run build",
		"deploy": "gh-pages -d build",
		"eject": "react-scripts eject",
		"flow": "flow",
		"lint": "eslint ./src/**/*.js[x] && flow",
		"generate": "graphql-codegen --config codegen.yml",
		"storybook": "start-storybook -p 6006 -s public",
		"build-storybook": "build-storybook -s public"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"devDependencies": {
		"@babel/cli": "7.16.0",
		"@babel/core": "7.16.0",
		"@babel/preset-flow": "7.16.0",
		"@graphql-codegen/cli": "^2.3.1",
		"@graphql-codegen/flow": "2.2.2",
		"@graphql-codegen/flow-operations": "2.2.2",
		"@graphql-codegen/flow-resolvers": "2.2.2",
		"@graphql-codegen/introspection": "2.1.1",
		"@react-mock/localstorage": "0.1.2",
		"@storybook/addon-actions": "^6.4.7",
		"@storybook/addon-essentials": "^6.4.7",
		"@storybook/addon-knobs": "^6.3.1",
		"@storybook/addon-links": "^6.4.7",
		"@storybook/node-logger": "^6.4.7",
		"@storybook/preset-create-react-app": "^4.0.0",
		"@storybook/react": "^6.4.7",
		"@testing-library/react": "12.1.2",
		"@testing-library/user-event": "^13.5.0",
		"babel-plugin-styled-components": "2.0.2",
		"eslint-config-airbnb": "19.0.4",
		"eslint-config-prettier": "8.3.0",
		"eslint-plugin-flowtype": "8.0.3",
		"eslint-plugin-import": "2.25.3",
		"eslint-plugin-jest-dom": "^4.0.0",
		"eslint-plugin-jsx-a11y": "6.5.1",
		"eslint-plugin-prettier": "4.0.0",
		"eslint-plugin-react": "7.28.0",
		"eslint-plugin-react-hooks": "4.3.0",
		"flow-bin": "0.168.0",
		"flow-typed": "^3.6.0",
		"gh-pages": "3.2.3",
		"husky": "7.0.4",
		"lint-staged": "11.2.6",
		"prettier": "2.4.1",
		"react-test-renderer": "^17.0.2",
		"storybook-addon-apollo-client": "^4.0.9"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push": "CI=true npm run test"
		}
	},
	"lint-staged": {
		"*.+(js|jsx)": [
			"eslint src/**/*.js[x] --fix"
		],
		"*.+(js|jsx|json|css|md)": [
			"prettier --write"
		]
	}
}
