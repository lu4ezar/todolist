{
	"name": "todolist",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@apollo/client": "3.7.12",
		"@emotion/react": "^11.10.6",
		"@emotion/styled": "^11.10.6",
		"@mui/icons-material": "^5.11.16",
		"@mui/lab": "^5.0.0-alpha.126",
		"@mui/material": "^5.12.0",
		"@mui/styled-engine-sc": "^5.12.0",
		"@testing-library/jest-dom": "5.16.5",
		"formik": "^2.2.9",
		"graphql": "^16.6.0",
		"jwt-decode": "^3.1.2",
		"luxon": "^3.3.0",
		"phosphor-react": "^1.4.1",
		"react": "18.2.0",
		"react-beautiful-dnd": "13.1.1",
		"react-dom": "18.2.0",
		"react-scripts": "5.0.1",
		"styled-components": "^5.3.9",
		"uuid": "9.0.0"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test --env=jsdom",
		"predeploy": "npm run generate && npm run build",
		"deploy": "gh-pages -d build",
		"eject": "react-scripts eject",
		"flow": "flow",
		"lint": "eslint ./src/**/*.js[x] && flow",
		"generate": "graphql-codegen --config codegen.yml",
		"storybook": "start-storybook -p 6006 -s public",
		"build-storybook": "build-storybook -s public"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"devDependencies": {
		"@babel/cli": "7.21.0",
		"@babel/core": "7.21.4",
		"@babel/preset-flow": "7.21.4",
		"@graphql-codegen/cli": "^3.3.0",
		"@graphql-codegen/flow": "2.3.6",
		"@graphql-codegen/flow-operations": "2.3.6",
		"@graphql-codegen/flow-resolvers": "2.4.4",
		"@graphql-codegen/introspection": "3.0.1",
		"@react-mock/localstorage": "0.1.2",
		"@storybook/addon-actions": "^7.0.2",
		"@storybook/addon-essentials": "^7.0.2",
		"@storybook/addon-knobs": "^7.0.0",
		"@storybook/addon-links": "^7.0.2",
		"@storybook/addon-storyshots": "^7.0.2",
		"@storybook/builder-webpack5": "^7.0.2",
		"@storybook/manager-webpack5": "^6.5.16",
		"@storybook/node-logger": "^7.0.2",
		"@storybook/preset-create-react-app": "^7.0.2",
		"@storybook/react": "^7.0.2",
		"@testing-library/react": "14.0.0",
		"@testing-library/user-event": "^14.4.3",
		"babel-plugin-styled-components": "2.1.1",
		"eslint-config-airbnb": "19.0.4",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-flowtype": "8.0.3",
		"eslint-plugin-import": "2.27.5",
		"eslint-plugin-jest-dom": "^4.0.3",
		"eslint-plugin-jsx-a11y": "6.7.1",
		"eslint-plugin-prettier": "4.2.1",
		"eslint-plugin-react": "7.32.2",
		"eslint-plugin-react-hooks": "4.6.0",
		"flow-bin": "0.203.1",
		"flow-typed": "^3.8.0",
		"gh-pages": "5.0.0",
		"husky": "8.0.3",
		"lint-staged": "13.2.1",
		"prettier": "2.8.7",
		"react-test-renderer": "^18.2.0",
		"storybook-addon-apollo-client": "^4.1.4",
		"webpack": "^5.76.3"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push": "CI=true npm run test"
		}
	},
	"lint-staged": {
		"*.+(js|jsx)": [
			"eslint src/**/*.js[x] --fix"
		],
		"*.+(js|jsx|json|css|md)": [
			"prettier --write"
		]
	}
}
